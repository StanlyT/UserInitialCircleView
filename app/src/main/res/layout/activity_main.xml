<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.example.asus.userinitialcircleview.UserInitialCircleView
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/test_circle_view"
        android:layout_width="200dp"
        android:layout_height="200dp"
        app:initial_text_circle_view="OK"
        app:initial_size_circle_view="12dp"
        app:fill_radius_circle_view="0.9"
        app:stroke_width_circle_view="5"
        app:shadow_color_circle_view="#bfbfbf"
        app:main_stroke_color_circle_view="#9b9b9b"
        app:initial_color_circle_view="#585858"
        app:inner_color_circle_view="#FFFFFF"/>
</RelativeLayout>